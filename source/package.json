{
  "name": "cdk-solution",
  "version": "v1.2.2",
  "description": "Synthesize templates for Secure Media Delivery at the Edge on AWS using AWS Cloud Development Kit (CDK).",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "bin": {
    "cdk-solution": "bin/secure_media_stream.js"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "npx tsc",
    "watch": "npx tsc -w",
    "test": "jest --coverage",
    "clean": "rm -rf node_modules/ dist/ coverage/ package-lock.json",
    "lint": "npx eslint .",
    "audit": "npm audit && npx cdk synth | cfn_nag",
    "bootstrap": "npx cdk bootstrap",
    "deploy": "npx cdk deploy --all",
    "wizard": "node dist/bin/wizard/index.js"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.409.0",
    "@aws-sdk/client-dynamodb": "^3.409.0",
    "@aws-sdk/client-lambda": "^3.409.0",
    "@aws-sdk/client-secrets-manager": "^3.409.0",
    "@aws-sdk/client-ssm": "^3.409.0",
    "@aws-sdk/client-wafv2": "^3.409.0",
    "@aws-sdk/credential-providers": "^3.409.0",
    "@aws-sdk/lib-dynamodb": "^3.409.0",
    "@types/babel__traverse": "7.18.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "adm-zip": "^0.5.10",
    "aws-cdk": "^2.125.0",
    "aws-sdk-client-mock": "^2.2.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-cdk/aws-servicecatalogappregistry-alpha": "^2.125.0-alpha.0",
    "aws-cdk-lib": "^2.125.0",
    "constructs": "^10.0.0",
    "joi": "^17.6.0",
    "prompts": "^2.4.2",
    "source-map-support": "^0.5.16"
  }
}
